<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Trails</title>
</head>
<body>
    <h1>List of Trails</h1>
    <ul>
        <% trails.forEach(function(trail) { %>
            <li><%= trail.trail %> - <%= trail.rating %> (<%= trail.description %>)</li>
        <% }); %>
    </ul>
</body>
</html>
